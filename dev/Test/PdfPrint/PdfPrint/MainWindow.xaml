<Window x:Class="PdfPrint.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PdfPrint"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="CKG PDF Printer" SizeToContent="WidthAndHeight">
    <StackPanel Margin="20">
        
        <Button Padding="15" Command="{Binding PrintCommand}">
            <StackPanel Orientation="Horizontal">
            <ContentPresenter Content="{StaticResource image/16x16/pdf}" />
            <TextBlock Margin="5,0,0,0">PDF drucken</TextBlock>
            </StackPanel>
        </Button>
        
    </StackPanel>
</Window>
