<%@ Master Language="VB" AutoEventWireup="false" CodeBehind="AppMaster.Master.vb" Inherits="AppChrysler.AppMaster" %>

<%@ Register Assembly="AjaxControlToolkit" Namespace="AjaxControlToolkit" TagPrefix="ajaxToolkit" %>
<%@ Register TagPrefix="uc1" TagName="menue" Src="../../../Menue/ServicesMenue.ascx" %>


<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head id="Head1" runat="server">
    <meta http-equiv="content-type" content="text/html; charset=iso-8859-1" />
    <script src="/Services/JScript/Jquery/jquery-1.8.0.min.js" type="text/javascript"></script>
    <script src="/Services/JScript/services.js" type="text/javascript"></script>
  </head>
<body id="top">
    <form id="Form1" runat="server">
    <asp:ScriptManager ID="Scriptmanager1" runat="server" EnableScriptGlobalization="true" EnableScriptLocalization="true" AsyncPostBackTimeout="36000" >
    </asp:ScriptManager>
     <asp:UpdateProgress ID="UpdateProgress1" runat="server" >
        <ProgressTemplate>
            <div class="divProgress" >

           </div>
            <div class="divWait">
                <table cellpadding="2" cellspacing="2">

                        <tr>
                            
                            <td>
                              <asp:Image ID="Image1" runat="server" ImageUrl="../../../Images/indicator.gif" ImageAlign="Middle" /><br />
                            </td>
                            <td  align="left">
                                <asp:Label ID="Label1" runat="server" CssClass="WaitControl" Text="Bitte warten..."
                                 Font-Size="14px" Font-Bold="True"></asp:Label>
                            </td>
                        </tr>
                        <tr>
                            <td></td>
                        
                            <td align="left"><asp:Label ID="Label2" runat="server" CssClass="WaitControl" Text="Ihr Vorgang wird bearbeitet. Dies kann einige Zeit in Anspruch nehmen."
                                 Font-Size="12px"></asp:Label></td>
                        </tr>
             
                   </table>

            </div>
        </ProgressTemplate>
    </asp:UpdateProgress>  
    
    <div id="website">
        <div id="header">
        <asp:Image ID="imgLogo" ImageUrl="../../../Images/logoDAD.jpg" alt="DAD Logo" class="dadLogo2"
                runat="server" />
            <div id="PlaceHeader" runat="Server" class="dvHead">
            </div>
            
            <ul id="navigation">
                <li runat="server" id="tdHauptmenue">
                    <asp:HyperLink ID="lnkHauptmenue" runat="server" NavigateUrl="../../../Start/Login.aspx">Startseite</asp:HyperLink></li>
                <li runat="server" id="tdChangePasword">
                    <asp:HyperLink ID="lnkChangePassword" runat="server" Visible="False" NavigateUrl="../../../Start/ChangePassword.aspx">Passwort ändern</asp:HyperLink></li>
                <li id="tdHandbuch" runat="server">
                    <asp:HyperLink ID="lnkHandbuch" runat="server" Target="_blank">Handbuch</asp:HyperLink></li>
                <li id="tdContact" runat="server">
                    <asp:HyperLink ID="lnkContact" NavigateUrl="../../../Info/ContactPage.aspx" runat="server">Kontakt</asp:HyperLink></li>
                <li id="tdResponsible" runat="server">
                    <asp:HyperLink ID="lnkResponsible" NavigateUrl="../../../Info/ResponsiblePage.aspx"
                        runat="server">Anprechpartner</asp:HyperLink></li>
                <li id="tdImpressum" runat="server">
                    <asp:HyperLink ID="lnkImpressum" NavigateUrl="../../../Info/Impressum.aspx" runat="server">Impressum</asp:HyperLink></li>
            </ul>
            <ul id="userinfo">
                <li>
                    <asp:Label ID="lblBenutzer" runat="server" ></asp:Label>
                    <span class="bold">
                        <asp:Label ID="lblUserName" runat="server"></asp:Label></span></li>
                <li class="logout">
                    <asp:HyperLink ID="lnkLogout" runat="server" 
                        ImageUrl="../../../Images/navLogout.gif" 
                        NavigateUrl="../../../Start/Logout.aspx" Visible="False">HyperLink</asp:HyperLink>
                </li>
            </ul>
        </div>
        <div id="innerContentLeft" runat="server">
            <div id="sidebar">
                <uc1:menue ID="Menue1" runat="server" />
            </div>
        </div>
        <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
        </asp:ContentPlaceHolder>
    </div>
    <div id="websitefoot">
        <div id="footfoot">
            <div id="footer">
                 <div class="leftAlign">
                     <asp:Label ID="lblCopyright" runat="server" Text="© 2001 &#8211; year DAD Deutscher Auto Dienst GmbH / Christoph Kroschke GmbH"></asp:Label>
                     </div>
                <div class="rightAlign">
                    <a href="#top">Top
                        <img src="../../../Images/top.gif" alt="nach oben" /></a></div>
            </div>
        </div>
    </div>
    </form>
</body>
</html>