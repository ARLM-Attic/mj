    function ShowEquiHistorieVermieterDetail(result) {      
        $("#divHistorieVermieterDetail").html(result);
        $("#divGrid").slideUp();
        $("#divHistorieVermieterDetail").slideDown();
        $("#btnBackToResult").show();
        setTimeout('LoadingHide()', 1000);
        FormPreparePrivateAjax();
    }

    function CloseEquiHistorieVermieterDetail() {
        //$("#btnBackToResult").hide();
        $("#divHistorieVermieterDetail").slideUp(function () { $("#divGrid").delay(200).slideDown(); window.scrollTo(0, 0); });
        $("#divGrid").slideDown();
       
    }

