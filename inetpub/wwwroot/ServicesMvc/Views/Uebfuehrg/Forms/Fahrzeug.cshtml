@model CkgDomainLogic.Uebfuehrg.Models.Fahrzeug 

<div id="@Ajax.AutoFormWrapperDivID(Model.UiIndex)" class="margin-top-20">
    @using (Ajax.AutoForm(Model, "Uebfuehrg", Model.UiIndex))
    {
        @Html.FormValidationSummary()
        
        @Html.HiddenFor(m => m.UiIndex)

        @Html.FormTextBoxFor(m => m.FIN, new { @class = "m-wrap medium" })

        @Html.FormTextBoxFor(m => m.Fahrzeugklasse, new { @class = "m-wrap medium" })

        @Html.FormTextBoxFor(m => m.Kennzeichen, new { @class = "m-wrap small" })
        

        @Html.FormTextBoxFor(m => m.Fahrzeugwert, new { @class = "m-wrap medium" })
        @Html.FormTextBoxFor(m => m.Typ, new { @class = "m-wrap medium" })
        @Html.FormTextBoxFor(m => m.FahrzeugZugelassen, new { @class = "m-wrap medium" })
        @Html.FormTextBoxFor(m => m.ZulassungsauftragAnDAD, new { @class = "m-wrap medium" })
        @Html.FormTextBoxFor(m => m.Bereifung, new { @class = "m-wrap medium" })

        
        @Html.FormRequiredFieldsSummary()
        
        
        <script type="text/javascript">

            _modelIsValid = @ViewContext.ViewData.ModelState.IsValid.ToString().ToLower();
        
        </script>
    }
</div>
