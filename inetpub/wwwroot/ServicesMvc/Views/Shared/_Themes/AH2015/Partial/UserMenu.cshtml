@model CkgDomainLogic.General.Contracts.ILogonContextDataService

@if (Model != null)
{
    <ul class="nav pull-right">
        @{ViewBag.HeaderIconCssClass = "icon-globe";}
        @Html.Partial("Partial/UserLanguageSwitch")

        @if (Model.MaintenanceInfo != null)
        {
            @Html.Partial("Partial/UserMenuMessagesDropdown", Model.MaintenanceInfo)
        }

        @Html.Partial("Partial/UserMenuUserInfoDropdown")

        <li class="logout"><a href="javascript:UserLogout()">Log Out</a></li>
    </ul>
}
