@using CkgDomainLogic.General.Database.Models
@using CkgDomainLogic.Autohaus.Models
@model FahrzeugakteDocument

<div class="form-horizontal">
    @Html.FormDropDownListFor(m => m.CategoryID, (ViewBag.AuswahlKategorie as List<CustomerDocumentCategory>).ToSelectList("ID", "CategoryName"), new { @class = "m-wrap" })
    
    @Html.FormTextBoxFor(m => m.Bemerkung, new { @class = "m-wrap" })
</div>

<div class="space20"></div>

<div id="divFahrzeugakteDocUpload">
    @Html.Partial("Fahrzeugverwaltung/Fahrzeugakte/Partial/Dokumente/FahrzeugakteDocUpload", Model)
</div>

