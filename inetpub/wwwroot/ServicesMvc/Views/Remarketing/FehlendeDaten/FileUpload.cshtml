@model CkgDomainLogic.Remarketing.Models.FehlendeDatenSelektor

<div class="control-group">
    <label class="control-label">@(Model.Auswahl == "F" ? Localize.UploadChassisNumbers : Localize.UploadLicenseNumbers):</label>
    <div class="controls">
                
        @(Html.Telerik().Upload()
                .Name("fileUpload")
                .Multiple(false)
                .Localizable("de-DE")
                .Async(async => async
                    .Save("ExcelUploadStart", "Remarketing")
                    .SaveField("uploadFiles")
                    .AutoUpload(true))
                .Multiple(false)
                .ShowFileList(false)
                .ClientEvents(events => events
                    .OnUpload("UploadStart")
                    .OnSuccess("UploadFinished")
                    .OnError("UploadError")
                )
            )

        <a href="DownloadFehlendeDatenExcelTemplate" class="btn white tooltips margin-left-5 margin-top-10" data-original-title="@Localize.DownloadTemplate" data-placement="top">@Localize.DownloadTemplate <i class="icon-chevron-right"></i></a>
    </div>
</div>      
