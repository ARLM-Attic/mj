@model CkgDomainLogic.CoC.Models.FreieZulassung

<div id="DivFreieZulassungsOptionen" class="margin-top-20">
    @using (Ajax.BeginForm("FreieZulassungsOptionenForm", "CocBeauftragung", null,
                new MvcAjaxOptions { UpdateTargetId = "DivFreieZulassungsOptionen", OnComplete = "AjaxFormFreieZulassungsOptionenComplete();" },
                    htmlAttributes: new { @class = "form-horizontal", id = "AjaxFormFreieZulassungsOptionen" }))
    {
        @Html.FormValidationSummary()
        

        @Html.FormTextBoxFor(m => m.AuftragsReferenz, new { @class = "m-wrap medium" })

        @Html.FormTextBoxFor(m => m.VIN, new { @class = "m-wrap small" })

        @Html.FormTextBoxFor(m => m.ZBII, new { @class = "m-wrap small" })

                
        @Html.FormRequiredFieldsSummary()
        
        
        <script type="text/javascript">

            _modelIsValid = @ViewContext.ViewData.ModelState.IsValid.ToString().ToLower();
        
        </script>
    }
</div>
