@model CkgDomainLogic.Fahrzeuge.Models.HolBringService.Anlieferung 
@using CkgDomainLogic.Autohaus.Models

<div id="DivReady" class="margin-top-20">
    @using (Ajax.BeginForm("Ready", "HolBringService", null,
                new MvcAjaxOptions { UpdateTargetId = "DivReady", OnComplete = "AjaxFormReadyComplete();" },
                    htmlAttributes: new { @class = "form-horizontal", id = "AjaxFormReady" }))
    {
        @Html.FormValidationSummary()

        <span>READY</span>

        @Html.FormRequiredFieldsSummary()
        
        <script type="text/javascript">
            _modelIsValid = @ViewContext.ViewData.ModelState.IsValid.ToString().ToLower();
        </script>
    }
</div>
