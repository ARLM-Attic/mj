@model CkgDomainLogic.Autohaus.ViewModels.KroschkeZulassungViewModel

<h4>@Localize.AccountHolderAddress</h4> 

<div class="form-actions">
    <a href="#" onclick="return AjaxFormAdressenShowGrid('Kontoinhaber')" class="btn white">@Localize.SelectAccountHolderAddress <i class="icon-chevron-right"></i></a>
